{% extends '::backend.html.twig' %}

{% block body -%}
<div class="page-header">
    <h1>Category</h1>
    <ul class="record_actions list-inline">
        <li>
            <a href="{{ path('admin_project_category') }}" class="btn btn-default">
                {{ "back to list"|trans }}
            </a>
        </li>
        <li>
            <a href="{{ path('admin_project_category_edit', { 'id': entity.id }) }}" class="btn btn-primary">
                {{ "edit"|trans }}
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
</div>
<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
    </tbody>
</table>


{% endblock %}

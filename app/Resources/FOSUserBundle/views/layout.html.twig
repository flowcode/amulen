{% extends '::backend.html.twig' %}

{% block body -%}
    {% for type, messages in app.session.flashbag.all() %}
        {% for key, message in messages %}
            <div class="flash-{{ type }}">
                {{ message|trans({}, 'FOSUserBundle') }}
            </div>
        {% endfor %}
    {% endfor %}

    {% block content %}
        {% block fos_user_content %}{% endblock %}
    {% endblock %}

{% endblock %}